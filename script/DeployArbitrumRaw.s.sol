pragma solidity ^0.8.22;

import {Script} from "forge-std/Script.sol";

contract DeployArbitrumRaw is Script {
    function run() external {
        uint256 deployerPrivateKey = vm.envUint("PRIVATE_KEY");
        address deployerAddress = vm.addr(deployerPrivateKey);
        
        console.log("Deployer address:", deployerAddress);
        
        // Arbitrum chain ID
        vm.chainId(42161);
        
        // Load Arbitrum RPC URL
        string memory rpcUrl = vm.envString("ARBITRUM_RPC_URL");
        
        // The deterministic deployer address
        address deployer = 0x4e59b44847b379578588920cA78FbF26c0B4956C;
        
        // The exact input data from Ethereum deployment
        bytes memory inputData = hex"";
        
        // Get the current nonce
        uint256 nonce = vm.getNonce(deployerAddress);
        console.log("Nonce:", nonce);
        
        // Get gas price
        uint256 gasPrice = block.basefee + 100000000; // base fee + 0.1 gwei priority
        
        // Create the transaction
        vm.broadcast(deployerPrivateKey);
        
        // Call the deployer with the input data
        (bool success, bytes memory returnData) = deployer.call{value: 0}(inputData);
        
        require(success, "Deployment failed");
        
        // Extract the deployed address from return data
        address deployedAddress;
        assembly {
            deployedAddress := mload(add(returnData, 0x20))
        }
        
        console.log("Deployed address:", deployedAddress);
    }
}

