# CreatorVault Frontend Environment Variables
# Copy this to .env.local for local development

# Required: WalletConnect Project ID
# Get yours at https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=

# Required: RPC URL (Base mainnet)
VITE_BASE_RPC=https://mainnet.base.org

# Optional: Database URL (Vercel Postgres or Neon)
DATABASE_URL=

# Optional: Cron job secret for sync endpoint security
CRON_SECRET=

# Contract Addresses (fill in after deployment)
VITE_VAULT_ADDRESS=
VITE_SHARE_OFT_ADDRESS=
VITE_WRAPPER_ADDRESS=
VITE_REGISTRY_ADDRESS=
