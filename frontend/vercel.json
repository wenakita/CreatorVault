{
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/__api/:path*", "destination": "/api/:path*" },
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "content-security-policy-report-only",
          "value": "default-src 'self'; script-src 'self' https://challenges.cloudflare.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: blob:; font-src 'self'; object-src 'none'; base-uri 'self'; form-action 'self'; frame-ancestors 'none'; child-src https://auth.privy.io https://verify.walletconnect.com https://verify.walletconnect.org; frame-src https://auth.privy.io https://verify.walletconnect.com https://verify.walletconnect.org https://challenges.cloudflare.com; connect-src 'self' https://auth.privy.io https://api.developer.coinbase.com wss://relay.walletconnect.com wss://relay.walletconnect.org wss://www.walletlink.org https://*.rpc.privy.systems https://explorer-api.walletconnect.com; worker-src 'self' blob:; manifest-src 'self'"
        },
        { "key": "referrer-policy", "value": "strict-origin-when-cross-origin" },
        { "key": "x-content-type-options", "value": "nosniff" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [{ "key": "cache-control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/.well-known/farcaster.json",
      "headers": [{ "key": "cache-control", "value": "no-store" }]
    },
    {
      "source": "/manifest.json",
      "headers": [{ "key": "cache-control", "value": "no-store" }]
    },
    {
      "source": "/explore/:path*",
      "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }]
    },
    { "source": "/swap", "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }] },
    { "source": "/positions", "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }] },
    { "source": "/portfolio", "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }] },
    { "source": "/dashboard", "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }] },
    { "source": "/admin", "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }] },
    { "source": "/admin/:path*", "headers": [{ "key": "x-robots-tag", "value": "noindex, nofollow" }] },
    { "source": "/index.html", "headers": [{ "key": "cache-control", "value": "no-store" }] }
  ]
}
