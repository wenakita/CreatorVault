{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install --production=false",
  "framework": "vite",
  "outputDirectory": "dist",
  "rootDirectory": "frontend",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "creatorvault.fun" }],
      "destination": "https://creatorvaults.fun/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.creatorvault.fun" }],
      "destination": "https://creatorvaults.fun/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.creatorvaults.fun" }],
      "destination": "https://creatorvaults.fun/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

